START:	LDA 2000H
	MVI B,00H	
	MVI C,00H	
	CPI 00H	
	JZ START	
L:	INR B			
	MOV D,A	
	MOV A,B	
	CPI 09H
	JNC LAST
	MOV A,D	
	RAL
	JC GO		
	JMP L
GO:	CALL DO_C	
	CALL PUT
LAST:	CALL FIX	
	JMP START	
ONE:	MOV D,A
	MVI A,01H
	CPI 05H
	MOV A,D
	RET
PUT:	MOV D,A
	MOV A,C
	CMA
	STA 3000H
	MOV A,D
	RET
DO_C:	MOV D,A
	MOV A,C
	CALL ONE	
	RAR
	MOV C,A	
	MOV A,D
	DCR B		
	JNZ DO_C
	RET
FIX:	LDA 2000H
	CPI 00H
	JNZ END
	MVI A,FFH
	STA 3000H
END:	RET

END